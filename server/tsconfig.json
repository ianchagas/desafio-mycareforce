{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2022",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "allowJs": true,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "skipDefaultLibCheck": true,
    "paths": {
      "@app/usuarios": [
        "libs/usuarios/src"
      ],
      "@app/usuarios/*": [
        "libs/usuarios/src/*"
      ],
      "@app/login": [
        "libs/login/src"
      ],
      "@app/login/*": [
        "libs/login/src/*"
      ],
      "@app/redis": [
        "libs/redis/src"
      ],
      "@app/redis/*": [
        "libs/redis/src/*"
      ],
      "@app/shared": [
        "libs/shared/src"
      ],
      "@app/shared/*": [
        "libs/shared/src/*"
      ],
      "@app/dados": [
        "libs/dados/src"
      ],
      "@app/dados/*": [
        "libs/dados/src/*"
      ]
    }
  }
}