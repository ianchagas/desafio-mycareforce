{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/usuarios/(.*)": "<rootDir>/../libs/usuarios/src/$1",
    "@app/usuarios": "<rootDir>/../libs/usuarios/src",
    "@app/login/(.*)": "<rootDir>/../libs/login/src/$1",
    "@app/login": "<rootDir>/../libs/login/src",
    "@app/redis/(.*)": "<rootDir>/../libs/redis/src/$1",
    "@app/redis": "<rootDir>/../libs/redis/src"
  }
}